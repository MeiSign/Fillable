# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Fillable API
GET   /                           controllers.ApplicationApi.status
GET   /complete/:indexName/:toBeCompleted	controllers.ApplicationApi.getCompletions(indexName: String, toBeCompleted: String)
GET		/status						controllers.ApplicationApi.status
GET   /status/requirements  controllers.ApplicationApi.checkRequirements
POST	/addCompletion/:indexName/:completion  controllers.ApplicationApi.addCompletion(indexName: String, completion: String)

# Admin Backend
GET		/admin						controllers.Backend.index

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
